              <stringProp name="script">import groovy.*;
import groovy.lang.*;
import org.json.JSONArray;
import org.json.JSONObject;
import groovy.json.JsonSlurper;

int threadNum = ctx.getThreadNum();
log.info(&quot;--------NGOCVK log current threadNum ------------&quot; + threadNum);

String jsonString = props.get(&quot;response&quot;);
log.info(&quot;--------NGOCVK log jsonString ------------&quot; + jsonString);

def jsonSlurper = new JsonSlurper();
def data = jsonSlurper.parseText(jsonString);
log.info(&quot;--------NGOCVK log data login ------------&quot; + data);

int size = data.staffId.size();
log.info(&quot;--------NGOCVK log size ------------&quot; + size);

String email = data.staffId.get(threadNum).get(&quot;email&quot;);
log.info(&quot;--------NGOCVK log email &quot; + threadNum + &quot;------------&quot; + email);
vars.put(&quot;email&quot;, email);
</stringProp>
